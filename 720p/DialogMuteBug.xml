<!-- Foundation -->
<window id="113">

	<defaultcontrol always="true">901</defaultcontrol>
	<allowoverlay>yes</allowoverlay>
	
	<controls>
	
		
			<animation type="WindowOpen" condition="!Window.Previous(mutebug)">
		<effect type="fade" start="0" end="100" time="100" />
	</animation>
	
	<animation type="WindowClose" condition="!Window.Next(mutebug)">
		<effect type="fade" start="100" end="0" time="400" />
	</animation>
	
		<animation type="Visible">
		<effect type="fade" start="0" end="100" time="100" />
	</animation>
	
	<animation type="Hidden">
		<effect type="fade" start="100" end="0" time="400" />
	</animation>
	

	
		<control type="group">
			
			<visible>!Window.IsVisible(104) + !Window.IsVisible(1212) + !Window.IsVisible(5545)</visible>
			
			<posx>750</posx>
			<posy>50</posy>
			<width>100</width>
			<height>145</height>
			
		
		
			<control type="image">
				
				<posx>198</posx>
				<posy>-10</posy>
				<width>200</width>
				<height>60</height>
				<texture>colors/color_white.png</texture>
				<colordiffuse>$INFO[Skin.String(gamercolor)]</colordiffuse>
				
			</control>
			
			
				<control type="label">
				<posx>225</posx>
				<posy>10</posy>
					<width>280</width>
					<height>20</height>
					<label>        Silenciado</label>
					<font>Settings-Label2</font>s
					<textcolor>Label_Normal</textcolor>
					<aligny>center</aligny>
				
				</control>
			
		</control>

	</controls>

</window>